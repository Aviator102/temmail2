{
  "version": 2,
  "builds": [
    {
      "src": "app_melhorado.py",
      "use": "@vercel/python"
    },
    {
      "src": "index_melhorado.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/gerar_conta",
      "dest": "app_melhorado.py"
    },
    {
      "src": "/verificar_emails/(.*)",
      "dest": "app_melhorado.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index_melhorado.html"
    }
  ]
}
